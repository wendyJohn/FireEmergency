<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="textFieldStyle" format="reference"/>
    <!--加载进度-->
    <declare-styleable name="ProgressWheel">
        <attr name="matProg_progressIndeterminate" format="boolean" />
        <attr name="matProg_barColor" format="color" />
        <attr name="matProg_rimColor" format="color" />
        <attr name="matProg_rimWidth" format="dimension" />
        <attr name="matProg_spinSpeed" format="float" />
        <attr name="matProg_barSpinCycleTime" format="integer" />
        <attr name="matProg_circleRadius" format="dimension" />
        <attr name="matProg_fillRadius" format="boolean" />
        <attr name="matProg_barWidth" format="dimension" />
        <attr name="matProg_linearProgress" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MarqueeViewStyle">
        <attr name="mvInterval" format="integer|reference" />
        <attr name="mvAnimDuration" format="integer|reference" />
        <attr name="mvTextSize" format="dimension|reference" />
        <attr name="mvTextColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="TabIndicatorView">
        <attr name="tabTextSize" format="integer|reference" />
        <attr name="tabCornerRadiu" format="integer|reference" />
        <attr name="tabBorderWidth" format="integer|reference" />
        <attr name="tabCheckedTextColor" format="color|reference" />
        <attr name="tabUnCheckedTextColor" format="color|reference" />
        <attr name="tabCheckBGcolor" format="color|reference" />
        <attr name="tabUnCheckBGcolor" format="color|reference" />
        <attr name="defaultCheckedPos" format="integer|reference" />
    </declare-styleable>

    <declare-styleable name="TabItemView">
        <attr name="tab_strokecheck_color" format="color|reference" />
        <attr name="tab_strokeuncheck_color" format="color|reference" />
        <attr name="tab_selected_color" format="color|reference" />
        <attr name="title_unselected_color" format="color|reference" />
        <attr name="title_selected_color" format="color|reference" />
        <attr name="tab_stroke_width" format="integer|reference" />
        <attr name="tab_textsize" format="integer|reference" />
        <attr name="tab_roundradiu" format="integer|reference" />
        <attr name="tab_title" format="string|reference" />
        <attr name="tab_position" format="integer|reference" />
        <attr name="tab_state" format="enum">
            <enum name="unselected" value="0" />
            <enum name="selected" value="1" />
        </attr>
    </declare-styleable>
</resources>